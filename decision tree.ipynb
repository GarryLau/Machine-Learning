    "from sklearn import tree\n",
    "X = [[0,0], [1,1]]\n",
    "Y = [0,1]\n",
    "clf = tree.DecisionTreeClassifier()\n",
    "clf = clf.fit(X, Y)                       #拟合X，Y\n",
    "pre = clf.predict([[2,2]])                #给定X预测其Y值\n",
    "print(pre)\n",
    "pro = clf.predict_proba([[2,2]])          #计算[2,2]分别属于0类的概率和属于1类的概率\n",
    "print(pro)"
   ]
  }
